{
  "name": "Sensor",
  "plural": "sensors",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": true
  },
  "properties": {
    "nome": {
      "type": "string"      
    },
    "valor_limite_min": {
      "type": "number",
      "postgresql": {
        "dataType": "double precision"
      }      
    },
    "valor_limite_max": {
      "type": "number",
      "postgresql": {
        "dataType": "double precision"
      }      
    }
  },
  "validations": [],
  "relations": {    
    "medida": {
      "type": "belongsTo",
      "model": "Medida",
      "foreignKey": "med_id"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}